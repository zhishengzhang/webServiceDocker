<?xml version="1.0" encoding="UTF-8"?>
<ehcache updateCheck="false" name="hibernateCache">
	<diskStore path="java.io.tmpdir/ehcache/subscribe" />
	<!-- DefaultCache setting. -->
	<!-- 默认使用LRU最近最少使用策略memoryStoreEvictionPolicy -->
	<defaultCache eternal="false" maxElementsInMemory="3000"
		overflowToDisk="false" diskPersistent="false" timeToIdleSeconds="0"
		timeToLiveSeconds="900" />
	<!-- Special objects setting. -->
	<cache name="SchoolClassesRelation" eternal="false"
		maxElementsInMemory="1000" overflowToDisk="false" diskPersistent="false"
		timeToIdleSeconds="0" timeToLiveSeconds="1200" />
</ehcache>